ourcesContent":["var render = function render() {\n  var _vm = this,\n    _c = _vm._self._c\n  return _c(\n    \"ContentSlotsDistributor\",\n    { attrs: { \"slot-key\": _vm.$parent.slotKey } },\n    [\n      _c(\"h1\", { attrs: { id: \"语句摘抄\" } }, [\n        _c(\n          \"a\",\n          { staticClass: \"header-anchor\", attrs: { href: \"#语句摘抄\" } },\n          [_vm._v(\"#\")]\n        ),\n        _vm._v(\" 语句摘抄\"),\n      ]),\n      _vm._v(\" \"),\n      _c(\"ol\", [\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"我与众不同，我很独特。无论我外表看起来多么普通，但在心灵深处，我深知自己是独一无二的奇迹。\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"不可能一次就中，师兄都没有一次就中，就算不中，也可以换个故事重新投；要求做实验的话，再自己多思考，多交流，网上查找资料进行回复\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"要更好实现自身价值，一是始终保持学习和研究事物的热情；二是至少找准一个领域持之以恒、久久为功。\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_c(\"p\", [_vm._v(\"归纳，总结，思考\")])]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"5W1H法则，为什么用法要注意，因为要求回答者追求答案的逻辑性。\"\n            ),\n          ]),\n          _vm._v(\" \"),\n          _c(\"ul\", [\n            _c(\"li\", [_vm._v(\"站在对方的立场，换位思考\")]),\n            _vm._v(\" \"),\n            _c(\"li\", [_vm._v(\"提容易回答的问题\")]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"请求别人帮忙的时候，要是能给出一个理由，成功的概率更大。这个理由如果能引起对方的共鸣，那就更加完美\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"人们选择那些与自身利益和情绪密切相关的事件运用理智思考，而忽视那些不感兴趣不相干的事凭借本能自动处理。要选择重要的问题并且投入精力，比处理问题还重要。\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_c(\"p\", [_vm._v(\"交情不够，少说几句\")])]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\"学会聆听，学会发现对方感兴趣的话题，引导对方说下去\"),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_c(\"p\", [_vm._v(\"避免争论，勇于认错，学会赞美\")])]),\n        _vm._v(\" \"),\n        _c(\"li\", [_c(\"p\", [_vm._v(\"真心实意感谢他人\")])]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [_vm._v(\"你的心界愈空灵，你也不觉得物界喧嘈，所谓忙中静趣\")]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"人的乐趣一半得之活动（行动起来，诸如运动，清理，培养兴趣等，闲人大半易于发愁），也还有一半得之于感受（欣赏美，感受风景，感受细微，感受生活）\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"烂开始，短平快（按周期步骤分解，减小阻力），多迭代（设定每轮迭代的目标）\"\n            ),\n          ]),\n          _vm._v(\" \"),\n          _c(\"blockquote\", [\n            _c(\"p\", [\n              _c(\n                \"a\",\n                {\n                  attrs: {\n                    href: \"https://www.zhihu.com/question/19979300/answer/1277084404?utm_campaign=shareopn&utm_medium=social&utm_oi=1224007786616672256&utm_psn=1734936480705961984&utm_source=wechat_session\",\n                    target: \"_blank\",\n                    rel: \"noopener noreferrer\",\n                  },\n                },\n                [_vm._v(\"提升执行力\"), _c(\"OutboundLink\")],\n                1\n              ),\n            ]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_c(\"p\", [_vm._v(\"及格是一种心态\")])]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _vm._v(\n              \"你没喜欢每一个人的义务，你有厌恶任何一个人的理由，但对每一个人，你都要给予基本的尊重，包括你厌恶的人\"\n            ),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _c(\"strong\", [\n              _vm._v(\n                \"如果你的亲人是普通阶层，那对于人生中一些大事来说，他们给的建议往往就是普通阶层的思维，他们的阶层就是他们一生思维决策的结果，如果你的目标是跳出本阶层，那最好只把他们的建议当成参考。\"\n              ),\n            ]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _c(\"strong\", [\n              _vm._v(\n                \"不要和你的同事比工资，没有意义，比工资总会有人受伤，更多的是负面影响，并且很多时候受伤的会是你。\"\n              ),\n            ]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _c(\"strong\", [\n              _vm._v(\n                \"不要说一项技能没有用，任何你掌握的技能都有价值，但是你要学会找到发挥它的场景。如果有一天你落水了，你可能会很庆幸，自己以前学会了游泳。\"\n              ),\n            ]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _c(\"strong\", [\n              _vm._v(\n                \"工作中如果要上升，你要勇于承担麻烦的、有挑战的任务，当你推掉麻烦的时候，你也推掉了机遇。\"\n              ),\n            ]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _c(\"strong\", [\n              _vm._v(\n                \"很多人喜欢不停的做事，但不会停下来思考，缺乏总结复盘的能力，其实阶段性总结复盘，不仅能够固化前面的经验，也能梳理后面的方向；把事情做对很重要，但是更重要的是做对的事；另外不要贪快，方向正确慢就是快\"\n              ),\n            ]),\n          ]),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _c(\"p\", [\n            _c(\"strong\", [\n              _vm._v(\n                \"不要因为某人的外在，如外貌、习惯、学历等对人贴上标签，去盲目否定别人，对于别人的建议，应该从客观出发，综合分析，从善如流是一项非常难得的品质。\"\n              ),\n            ]),\n          ]),\n        ]),\n      ]),\n    ]\n  )\n}\nvar staticRenderFns = []\nrender._withStripped = true\n\nexport { render, staticRenderFns }"],"mappings":"AAAA,IAAIA,MAAM,GAAG,SAASA,MAAMA,CAAA,EAAG;EAC7B,IAAIC,GAAG,GAAG,IAAI;IACZC,EAAE,GAAGD,GAAG,CAACE,KAAK,CAACD,EAAE;EACnB,OAAOA,EAAE,CACP,yBAAyB,EACzB;IAAEE,KAAK,EAAE;MAAE,UAAU,EAAEH,GAAG,CAACI,OAAO,CAACC;IAAQ;EAAE,CAAC,EAC9C,CACEJ,EAAE,CAAC,IAAI,EAAE;IAAEE,KAAK,EAAE;MAAEG,EAAE,EAAE;IAAO;EAAE,CAAC,EAAE,CAClCL,EAAE,CACA,GAAG,EACH;IAAEM,WAAW,EAAE,eAAe;IAAEJ,KAAK,EAAE;MAAEK,I